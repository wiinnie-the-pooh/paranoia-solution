FoamFile
{
    version 2.0;
    format ascii;

    root ".";
    case "nuclear";
    instance "constant";
    local "";

    class dictionary;
    object nuclearCalculation;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

requestedPower  power [ 1 2 -3 0 0 0 0 ] 1111111.1;
powerIsFixed    false;
insertReactivity 0;
maxIterations   10;
tolerance       1e-07;


